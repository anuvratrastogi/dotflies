#!/usr/bin/env zsh

# -----
# vars
# -----

DOTFILES="$HOME/.dotfiles"
OS="${"$(uname)":l}"

# -----
# user-secret
# -----

# use .localrc for SUPER SECRET STUFF that you don't want in your public repo.
if [ -f $HOME/.localrc ]; then
  source $HOME/.localrc
fi

# -----
# import
# -----

source "$HOME"/.zshrc.user
source "$HOME"/.zshrc.theme
source "$HOME"/.zshrc.framework

# -----
# clear
# -----

clear